{
  "name": "bausenberg-check-in",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "typecheck": "vue-tsc --noEmit",
    "format": "prettier .  --write",
    "up": "taze -I"
  },
  "dependencies": {
    "@picocss/pico": "^1.5.6",
    "axios": "^1.1.3",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.3.7",
    "sweetalert2": "^11.6.4",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@types/node": "^18.11.7",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.6.0",
    "prettier": "^2.7.1",
    "rollup-plugin-visualizer": "^5.8.3",
    "taze": "^0.8.3",
    "typescript": "^4.8.4",
    "vite": "^3.2.0",
    "vue-tsc": "^1.0.9"
  }
}
