generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model CheckIn {
  id           String    @id @default(uuid())
  checkInDate  DateTime
  checkOutDate DateTime?
  club         String
  landing      String?
  name         String // match: /(\w|[üäöÄÜÖß-]){3,} (\w|[üäöÄÜÖß-]){3,}/,
}
